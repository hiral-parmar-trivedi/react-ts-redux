{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react"
  },
  "include": ["/src/main.tsx", "/src/login.tsx", "/src/join.tsx"],
  "exclude": [
    "public",
    "node_modules",
    "node_modules/@types",
    "node_modules/@types*",
    "/node_modules",
    "../node_modules",
    "*node_modules*",
    "/node_modules*",
    "/cypress",
    "src/__tests__"
  ]
}
